32 dict begin

/DX -5 def
/DY  3 def

/reversed true def

/MINangle DY DX atan def
/MAXangle DY DX atan 180 add def

/step 1.0 64.0 div def

/mv {
    /y0 exch def
    /x0 exch def
    /x x0 def
    /y y0 def
    newpath
} bind def

/ln {
    /yy exch def
    /xx exch def
    yy y sub
    xx x sub
    1 index 0 eq 1 index 0 eq and not {
        atan /a exch def
        reversed {
            a MINangle le
            a MAXangle ge
            or
        } {
            a MINangle ge
            a MAXangle le
            and
        } ifelse {
            a 45 sub
            2 mul
            cos
            1 add
            2.4 div
            setgray
            x y moveto
            xx yy lineto
            DX DY rlineto
            x DX add y DY add lineto
            closepath
            fill
        } if
    } if
    /x xx def
    /y yy def
} bind def

/cr {
    /y4 exch def
    /x4 exch def
    /y3 exch def
    /x3 exch def
    /y2 exch def
    /x2 exch def
    /x1 x def
    /y1 y def
    /cx x2 x1 sub 3 mul def
    /cy y2 y1 sub 3 mul def
    /bx x3 x2 sub 3 mul cx sub def
    /by y3 y2 sub 3 mul cy sub def
    /ax x4 x1 sub cx sub bx sub def
    /ay y4 y1 sub cy sub by sub def
    step step 1 {
	/t exch def
	t t mul t mul ax mul
	t t mul bx mul add
	t cx mul add
	x1 add
	t t mul t mul ay mul
	t t mul by mul add
	t cy mul add
	y1 add
	ln
    } for
} bind def

