/DrawLeaf
{
	//leafWidth //dLeafWidth ApplyVariation 1 scale
	0 //leafRadius //dLeafRadius ApplyVariation dup 0 360 arc
	.333 dLeafColor
	.85 .15 ApplyVariation
	.75 .25 ApplyVariation
	sethsbcolor
	//leafRB //dLeafColor ApplyVariation
	//leafGreen //dLeafColor ApplyVariation
	//leafRB //dLeafColor ApplyVariation
	setrgbcolor fill
} bind def


/Branch
{
	/depth depth 1 add store
	0 0 moveto
	0 dXY ApplyVariation y1 dXY ApplyVariation				% 1st control point
	0 dXY ApplyVariation y2 dXY ApplyVariation				% 2nd control point
	0 dXY ApplyVariation branchLength dXY ApplyVariation	% Far endpoint
	curveto
	currentpoint translate
	branchWidth dWidth ApplyVariation setlinewidth
	stroke
	depth maxDepth lt
	{
		gsave
			descendSizeFactor dup scale
			gsave
				angle dAngle ApplyVariation rotate Branch
			grestore
			gsave
				-angle dAngle ApplyVariation rotate Branch
			grestore
			0 dAngle ApplyVariation rotate
			1 .7 .3 ApplyVariation dup scale
			Branch
		grestore
	}
	{
		gsave
			descendSizeFactor dup scale
			0 dAngle ApplyVariation rotate
			DrawLeaf
		grestore
	}
	ifelse

	/depth depth 1 sub store
} bind def

